      <command:name>New-AppvSequencerPackage</command:name>
      <command:verb>New</command:verb>
      <command:noun>AppvSequencerPackage</command:noun>
      <maml:description>
        <maml:para>Creates a new App-V package.</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>The New-AppvSequencerPackage cmdlet creates a new Microsoft Application Virtualization (App-V) package, either using an installer, an App-V accelerator, or an accelerator with an installed application. The cmdlet accepts a template file, as well as the option to force the package to be fully streamed to the computer before running the package.</maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>New-AppvSequencerPackage</maml:name>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="False" position="0" aliases="none">
          <maml:name>Name</maml:name>
          <maml:description>
            <maml:para>Specifies the name of the App-V package. This is also the name of all files outputted by the sequencing process.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
          <dev:type>
            <maml:name>String</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>None</dev:defaultValue>
        </command:parameter>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="False" position="1" aliases="OutputPath">
          <maml:name>Path</maml:name>
          <maml:description>
            <maml:para>Specifies the folder where the package is saved.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
          <dev:type>
            <maml:name>String</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>None</dev:defaultValue>
        </command:parameter>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="False" position="2" aliases="none">
          <maml:name>AcceleratorFilePath</maml:name>
          <maml:description>
            <maml:para>Specifies the path to the accelerator file for this package. If the accelerator is not signed or is not accepted by the Sequencer, an error is returned.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
          <dev:type>
            <maml:name>String</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>None</dev:defaultValue>
        </command:parameter>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="False" position="3" aliases="none">
          <maml:name>InstallMediaPath</maml:name>
          <maml:description>
            <maml:para>Specifies the location of the installation media that the Sequencer points to and generates an accelerator.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
          <dev:type>
            <maml:name>String</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>None</dev:defaultValue>
        </command:parameter>
      </command:syntaxItem>
